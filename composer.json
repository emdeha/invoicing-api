{
  "name": "emdeha/invoicing-api",
  "type": "standard",
  "description": "An API for summing invoices",
  "homepage": "https://github.com/emdeha/invoicing-api",
  "license": "MIT License",
  "bin": ["bin/main"],
  "require": {
    "monolog/monolog": "^2.5",
    "slim/slim": "4.*",
    "slim/psr7": "^1.5",
    "phpspec/prophecy": "^1.15"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpunit/php-code-coverage": "^9.2",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "InvoicingAPI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "InvoicingAPI\\": "tests"
    }
  }
}
